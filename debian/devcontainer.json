{
  "name": "devcontainer-debian",
  "build": {
    "args": {
      "http_proxy": "${localEnv:http_proxy}",
      "https_proxy": "${localEnv:https_proxy}",
      "no_proxy": "${localEnv:no_proxy}",
      "DEBIAN_IMAGE": "docker.io/library/debian:bullseye",
      "DEBIAN_MIRROR_PATH": "${localEnv:DEBIAN_MIRROR_PATH}"
    },
    "dockerfile": "Dockerfile"
  },
  "containerEnv": {
    "http_proxy": "${localEnv:http_proxy}",
    "https_proxy": "${localEnv:https_proxy}",
    "no_proxy": "${localEnv:no_proxy}"
  },
  "customizations": {
    "vscode": {
      "settings": {},
      "extensions": [
        "EditorConfig.EditorConfig",
        "VisualStudioExptTeam.vscodeintellicode"
      ]
    }
  },
  "mounts": [
    "source=devcontainer-debian,target=/home/debian/.vscode-server/extensions,type=volume"
  ],
  "remoteUser": "debian"
}
